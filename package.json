{
  "version": "0.0.0-development",
  "name": "@committed/components-graph",
  "description": "Committed Graph Component",
  "author": "Committed",
  "license": "MIT",
  "private": false,
  "repository": "git://github.com/commitd/components-graph",
  "main": "dist/committed-components-graph.cjs.js",
  "module": "dist/committed-components-graph.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "commit": "cz",
    "build": "npm run build -ws",
    "lint": "npm run lint -ws",
    "test": "npm run test -ws",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "coverage": "npx live-server coverage/lcov-report",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "format": "prettier --write '**/{src,test,stories}/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}' example/index.tsx",
    "format:check": "prettier --check '**/{src,test,stories}/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}' example/index.tsx",
    "storybook": "start-storybook -p 6006 -h 0.0.0.0",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepare": "husky install",
    "precommit": "npm run precommit -ws",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@committed/components": ">= 6.0.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/rollpkg/configs/eslint"
    ],
    "plugins": [
      "import"
    ],
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true
        }
      }
    },
    "rules": {
      "import/default": "off",
      "import/no-default-export": "off",
      "react/display-name": "off",
      "import/no-named-as-default-member": "warn"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "**/*.stories.tsx",
      "**/*.test.ts",
      "**/*.test.tsx"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,mdx}": "prettier --write",
    "*.{ts,tsx}": [
      "npm run precommit"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@committed/components": "^6.3.0",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/n3": "^1.10.3",
    "@types/react-cytoscapejs": "^1.1.2",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^8.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollpkg": "^0.5.7",
    "semantic-release": "^17.4.4",
    "shx": "^0.3.2",
    "storybook-dark-mode": "^1.0.8",
    "tslib": "^2.0.3",
    "typescript": "^4.4.2"
  },
  "workspaces": [
    "packages/data",
    "packages/react",
    "packages/all"
  ]
}
