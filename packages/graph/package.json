{
  "name": "@committed/graph",
  "version": "0.0.0-development",
  "description": "Graph models, types and API",
  "author": "Committed",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commitd/components-graph.git"
  },
  "bugs": {
    "url": "https://github.com/commitd/components-graph/issues"
  },
  "homepage": "https://github.com/commitd/components-graph#readme",
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "@committed/config/jest/node"
  },
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "tsup --legacy-output && tsc --project tsconfig.build.json",
    "lint": "tsc && eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "precommit": "lint-staged",
    "dev": "tsup --legacy-output --watch",
    "fix": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "tsup": {
    "clean": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "target": "es2020",
    "minify": true,
    "sourcemap": true,
    "tsconfig": "./tsconfig.build.json"
  },
  "devDependencies": {
    "@committed/config": "*"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
