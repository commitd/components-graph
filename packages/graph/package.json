{
  "name": "@committed/graph",
  "version": "1.0.0",
  "description": "",
  "main": "dist/committed-graph.cjs.js",
  "module": "dist/committed-graph.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "rollpkg",
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/types/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/src/typings.d.ts",
      "test/*",
      ".*/index.ts",
      ".*.test.ts",
      ".*.test.tsx",
      ".*.stories.tsx"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "jest --bail --passWithNoTests"
    ]
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "rollpkg build --noStats --tsconfig ./tsconfig.build.json",
    "watch": "rollpkg watch",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commitd/components-graph.git"
  },
  "author": "Committed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commitd/components-graph/issues"
  },
  "homepage": "https://github.com/commitd/components-graph#readme",
  "dependencies": {
    "n3": "^1.11.1",
    "rdf-literal": "^1.3.0",
    "uuid": "^8.3.2"
  }
}
