{
  "name": "@committed/components-graph-react",
  "version": "0.0.0-development",
  "description": "Committed Components Graph for react",
  "author": "Committed",
  "private": false,
  "license": "MIT",
  "exports": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commitd/components-graph.git"
  },
  "bugs": {
    "url": "https://github.com/commitd/components-graph/issues"
  },
  "homepage": "https://github.com/commitd/components-graph#readme",
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "@committed/config/jest/jsdom"
  },
  "scripts": {
    "//prepare": "npm run codegen",
    "codegen": "panda codegen",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build:clean": "rm -rf dist",
    "build:css": "panda cssgen",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:js": "node ../../node_modules/@committed/config/esbuild/build.mjs",
    "build": "npm run build:clean && npm run build:css && npm run build:ts && npm run build:js",
    "lint": "tsc && eslint './src/**/*.{ts,tsx}'",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "precommit": "lint-staged",
    "fix": "TIMING=1 eslint src/**/*.ts* --fix"
  },
  "devDependencies": {
    "@committed/ds": "^0.1.0",
    "@committed/ds-ss": "^0.1.0",
    "@committed/config": "*",
    "@committed/graph": "*",
    "@mdi/js": "^7.2.96",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@committed/ds": "^0.1.0",
    "@committed/ds-ss": "^0.1.0",
    "@committed/graph": "*",
    "@mdi/js": ">= 7.2.96",
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  },
  "dependencies": {
    "cytoscape": "^3.23.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "react-cytoscapejs": "^2.0.0",
    "tinycolor2": "^1.4.2",
    "use-debounce": "^7.0.0"
  }
}
