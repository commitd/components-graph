{
  "name": "@committed/components-graph-react",
  "version": "1.0.0",
  "description": "Committed Compoennts Graph for react",
  "main": "dist/committed-components-graph-react.cjs.js",
  "module": "dist/committed-components-graph-react.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "preset": "rollpkg",
    "setupFiles": [
      "../../jest.patch-cssom.js"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleDirectories": [
      "node_modules",
      "utils"
    ],
    "testRegex": "/src/.*(\\.|/)(test|spec)\\.[jt]sx?$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/types/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/src/typings.d.ts",
      "setupTests.tsx",
      ".*/index.ts",
      ".*.test.ts",
      ".*.test.tsx",
      ".*.stories.tsx"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "jest --bail"
    ]
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "rollpkg build --noStats --tsconfig ./tsconfig.build.json",
    "watch": "rollpkg watch",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006 -h 0.0.0.0",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages --ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commitd/components-graph.git"
  },
  "author": "Committed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commitd/components-graph/issues"
  },
  "homepage": "https://github.com/commitd/components-graph#readme",
  "dependencies": {
    "@committed/components-graph-data": "*",
    "cytoscape-cola": "^2.4.0",
    "n3": "^1.11.1",
    "rdf-literal": "^1.2.0",
    "react-cytoscapejs": "^1.2.1",
    "tinycolor2": "^1.4.2",
    "typescript": "^4.4.4",
    "use-debounce": "^7.0.0",
    "uuid": "^8.3.1"
  }
}
